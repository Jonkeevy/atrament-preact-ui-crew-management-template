{
  "name": "@atrament/preact-ui",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "prestart": "node ink-compile.js",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "prebuild": "node ink-compile.js",
    "build": "preact build --no-esm --no-prerender",
    "serve": "sirv build --port 8900 --cors --single",
    "dev": "preact watch --port 8900",
    "analyze": "preact build --analyze --no-esm --no-prerender",
    "lint": "eslint src"
  },
  "dependencies": {
    "@atrament/web": "1.2.1",
    "@nanostores/preact": "0.5.1",
    "history": "5.3.0",
    "inkjs": "2.2.4",
    "normalize.css": "8.0.1",
    "preact": "10.22.0",
    "preact-router": "4.1.2",
    "seamless-scroll-polyfill": "2.3.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.24.5",
    "eslint": "8.49.0",
    "eslint-config-preact": "1.4.0",
    "eslint-plugin-compat": "4.2.0",
    "if-env": "1.0.4",
    "jest": "29.7.0",
    "preact-cli": "3.5.1",
    "remove-files-webpack-plugin": "1.5.0",
    "sirv-cli": "1.0.14"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
